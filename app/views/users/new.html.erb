<section class="user-signup">
  <div class="signup-template">
    <h1>Signup!</h1>
    <%= form_for :user, url: '/users' do |f| %>

      <div class="signup">
        Frist name <p><%= f.text_field :first_name %></p>
        Last name  <p><%= f.text_field :last_name %></p>
        Email <p><%= f.text_field :email %></p>
        Password <p><%= f.password_field :password %></p>
        Password Confirmation <p><%= f.password_field :password_confirmation %></p>
        <p><%= f.submit "Submit" %></p>
      </div>
    <% end %>
  </div>
</section>